{
  "name": "@hexafuchs/what.js",
  "version": "1.0.1",
  "homepage": "https://github.com/Hexafuchs/what.js",
  "contributors": [
    {
      "name": "Leo",
      "email": "leo@hexafuchs.de"
    }
  ],
  "keywords": [
    "Hexafuchs",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/Hexafuchs/what.js/issues"
  },
  "license": "MIT",
  "type": "module",
  "main": "./dist/what.cjs",
  "module": "./dist/what.mjs",
  "exports": {
    ".": {
      "require": "./dist/what.cjs",
      "import": "./dist/what.mjs"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite --host",
    "build": "rimraf --glob build/**/* && tsc && vite build && copyfiles ./package.json build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "lint:scripts": "eslint . --ext .ts,.js",
    "lint:styles": "stylelint ./**/*.{css,scss,sass}",
    "format:scripts": "prettier . --write",
    "format:styles": "stylelint ./**/*.{css,scss,sass} --fix --aei",
    "format": "npm run format:scripts && npm run format:styles",
    "docs": "typedoc"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^6.19.1",
    "@vitest/coverage-v8": "^1.2.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.33",
    "postcss-sass": "^0.5.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.2.4",
    "rimraf": "^6.0.1",
    "stylelint": "^16.2.0",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-sass-guidelines": "^11.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.0",
    "ts-node": "^10.9.2",
    "typedoc": "^0.26.5",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-dts": "^4.0.1",
    "vitest": "^1.2.1"
  }
}
