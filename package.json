{
  "name": "@hexafuchs/what.js",
  "version": "2.0.0",
  "homepage": "https://github.com/Hexafuchs/what.js",
  "contributors": [
    {
      "name": "Leo",
      "email": "leo@hexafuchs.de"
    }
  ],
  "keywords": [
    "Hexafuchs",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/Hexafuchs/what.js/issues"
  },
  "license": "MIT",
  "files": [
    "build"
  ],
  "type": "module",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "scripts": {
    "build": "rimraf --glob build/**/* && tsc && tsc-alias",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint . --ext .ts,.js",
    "format": "prettier . --write",
    "docs": "typedoc",
    "publish:locally": "npm config set ~/.prefix && npm link"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.12",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.29.0",
    "@vitest/coverage-v8": "^3.0.9",
    "copyfiles": "^2.4.1",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.11",
    "typedoc": "^0.28.1",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9"
  }
}
